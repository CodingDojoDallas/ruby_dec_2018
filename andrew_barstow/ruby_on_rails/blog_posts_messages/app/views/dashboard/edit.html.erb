<h1>Edit User <%=@user[0]["first_name"]%>!</h1>
<form action='user/create' method='post'>
	<label for='fname'>first_name</label>
	<input type='text' id='fname' name='fname' value='<%=@user[0]["first_name"]%>' class='fname'>
	<label for='lname'>last_name</label>
	<input type='text' id='lname' name='lname' value='<%=@user[0]["last_name"]%>' class='lname'>
	<label for='email'>email</label>
	<input type='text' id='email' name='email' value='<%=@user[0]["email_address"]%>' class='email'>
</form>