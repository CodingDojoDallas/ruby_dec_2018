<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
<div class="container">
  <form class="form-group" action="/dojos" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <label for="username">Branch:</label>
    <input class="form-control" type="text" name="dojo[branch]" value="">
    <label for="email">Street:</label>
    <input class="form-control" type="text" name="dojo[street]" value="">
    <label for="">City:</label>
    <input class="form-control" type="text" name="dojo[city]" value="">
    <label for="">State:</label>
    <input class="form-control" type="text" name="dojo[state]" value="">
    <button class="btn btn-primary" type="submit">Submit</button>
  </form>
  <a href="/dojos">Home</a>
</div>
