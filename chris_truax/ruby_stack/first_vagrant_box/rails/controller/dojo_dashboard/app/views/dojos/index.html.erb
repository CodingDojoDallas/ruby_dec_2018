<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
<div class="container">
    <h1>Listing <%=@dojocount%> Dojos</h1>
</div>
<table class="container table">
  <thead>
    <tr>
      <th scope="col">Coding Dojo Branch</th>
      <th scope="col">Street</th>
      <th scope="col">City</th>
      <th scope="col">State</th>
      <th scope="col">Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @dojos.each do |dojo| %>
    <tr>
      <th scope="row"><%=dojo.branch%></th>
      <td><%=dojo.street%></td>
      <td><%=dojo.city%></td>
      <td><%=dojo.state%></td>
      <td><a class="badge badge-primary" href="/dojos/<%=dojo.id%>">Show</a> | <a class="badge badge-success" href="/dojos/<%=dojo.id%>/edit">Edit</a> | <a class="badge badge-danger" href="/dojos/<%=dojo.id%>" data-method="delete">Destroy</a></td>
    </tr>
    <%end%>
  </tbody>
</table>
<div class="container">
  <a class="btn btn-primary" href="/dojos/new">New Dojo</a>
</div>