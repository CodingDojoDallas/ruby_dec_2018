<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
<div class="container">
    <h1>Listing <%=@studentcount%> students</h1>
</div>
<table class="container table">
        <thead>
            <tr>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">Email</th>
            <th scope="col">Actions</th>
            </tr>
        </thead>
        <tbody>
            <% @students.each do |student| %>
            <tr>
            <th scope="row"><%=student.first_name%></th>
            <td><%=student.last_name%></td>
            <td><%=student.email%></td>
            <td><a class="badge badge-primary" href="/dojos/<%=student.dojo_id%>/students/<%=student.id%>">Show</a> | <a class="badge badge-success" href="/dojos/<%=student.dojo_id%>/students/<%=student.id%>/edit">Edit</a> | <a class="badge badge-danger" href="/dojos/<%=student.dojo_id%>/students/<%=student.id%>" data-method="delete">Destroy</a></td>
            </tr>
            <%end%>
        </tbody>
    </table>
<div class="container">
  <a class="btn btn-primary" href="/students/new">New student</a>
</div>